		<value>/views/subject/messages</value>
		<value>/views/tutorial/messages</value>
		<value>/views/lectureNote/messages</value>
		<value>/views/slot/messages</value>
		<value>/views/privateMessage/messages</value>
		<value>/views/tutor/messages</value>
		<value>/views/teacher/messages</value>
		<value>/views/application/messages</value>
		<value>/views/comment/messages</value>
		<value>/views/lecture/messages</value>
		<value>/views/verification/messages</value>
	</util:list>

</beans>